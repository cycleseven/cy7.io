{
  "name": "cy7.io",
  "private": true,
  "description": "My cool website!!",
  "version": "0.1.0",
  "author": "Owen Gillespie <owen@cy7.io>",
  "sideEffects": false,
  "dependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-typescript": "^7.14.5",
    "@dangerfarms/eslint-config-df": "^6.2.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@stitches/react": "^1.0.0",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/builder-webpack5": "^6.3.7",
    "@storybook/manager-webpack5": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@types/jest": "^26.0.23",
    "@types/mdx-js__react": "^1.5.4",
    "@types/node": "^16.11.7",
    "@types/prismjs": "^1.16.6",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/typography": "^0.16.3",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-gatsby": "^0.12.1",
    "browserslist": "^4.16.3",
    "date-fns": "^2.17.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^27.0.0",
    "gatsby": "^4.24.8",
    "gatsby-plugin-extract-schema": "^0.2.0",
    "gatsby-plugin-graphql-codegen": "^3.0.0",
    "gatsby-plugin-image": "^2.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-mdx": "^2.13.0",
    "gatsby-plugin-no-javascript": "^2.0.5",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-pnpm": "^1.2.10",
    "gatsby-plugin-react-helmet": "^5.24.0",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-plugin-typography": "^4.24.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.25",
    "gatsby-remark-prismjs": "^5.10.0",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-transformer-remark": "^4.10.0",
    "gatsby-transformer-sharp": "^4.24.0",
    "jest": "^26.6.3",
    "lefthook": "^1.2.0",
    "prettier": "^2.7.1",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-innertext": "^1.1.5",
    "react-typography": "^0.16.19",
    "typescript": "^4.4.2",
    "typography": "^0.16.19",
    "webpack": "^5.51.1"
  },
  "engines": {
    "node": ">=16.0.0 <17.0.0",
    "pnpm": ">=7.0.0 <8.0.0"
  },
  "browserslist": [
    "defaults",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "test": "jest",
    "storybook": "start-storybook -p 8001 --docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cycleseven/cy7.io"
  },
  "eslintConfig": {
    "extends": [
      "@dangerfarms/eslint-config-df",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true,
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "import/extensions": [
        "warn",
        "ignorePackages",
        {
          "js": "never",
          "mjs": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "import/exports-last": "warn",
      "import/order": [
        "warn",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal",
              "parent",
              "sibling",
              "index"
            ]
          ],
          "newlines-between": "always"
        }
      ],
      "import/no-unresolved": "off",
      "react/prop-types": "off",
      "react/require-default-props": "off",
      "unicorn/prefer-type-error": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".mjs",
            ".js",
            ".json",
            ".ts",
            ".tsx"
          ]
        }
      },
      "import/extensions": [
        ".js",
        ".mjs",
        ".jsx",
        ".ts",
        ".tsx"
      ]
    },
    "overrides": [
      {
        "files": [
          "*.js",
          "*.jsx"
        ],
        "rules": {
          "@typescript-eslint/explicit-module-boundary-types": "off"
        }
      }
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)?$": "<rootDir>/jestPreprocess.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.cache",
      "/public/",
      "/storybook-static/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "watchPathIgnorePatterns": [
      "/.cache/",
      "/public/",
      "/generated/",
      "/storybook-static/",
      "schema.graphql"
    ]
  },
  "pnpm": {
    "overrides": {
      "@types/react": "17.0.11",
      "css-loader": "5.2.7",
      "webpack": "5.51.1"
    }
  },
  "prettier": {
    "endOfLine": "lf"
  }
}
