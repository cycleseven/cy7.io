{
  "name": "cy7.io",
  "private": true,
  "description": "My cool website!!",
  "version": "0.1.0",
  "author": "Owen Gillespie <owen@cy7.io>",
  "dependencies": {
    "@arkweid/lefthook": "0.6.3",
    "@babel/core": "7.6.4",
    "@dangerfarms/eslint-config-df": "5.0.0",
    "@storybook/addon-actions": "5.3.0-alpha.17",
    "@storybook/addon-docs": "5.3.0-alpha.17",
    "@storybook/react": "5.3.0-alpha.17",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "date-fns": "2.6.0",
    "eslint": "6.2.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-cypress": "2.6.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.15.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-unicorn": "10.0.0",
    "gatsby": "2.15.36",
    "gatsby-image": "2.2.27",
    "gatsby-plugin-manifest": "2.2.21",
    "gatsby-plugin-offline": "3.0.14",
    "gatsby-plugin-react-helmet": "3.1.11",
    "gatsby-plugin-sharp": "2.2.29",
    "gatsby-plugin-styled-components": "3.1.9",
    "gatsby-plugin-typography": "2.3.12",
    "gatsby-source-filesystem": "2.1.31",
    "gatsby-transformer-remark": "2.6.32",
    "gatsby-transformer-sharp": "2.2.21",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "styled-components": "4.4.0",
    "stylelint": "11.1.1",
    "stylelint-config-prettier": "6.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "typography": "0.16.19"
  },
  "engines": {
    "node": ">=12.9.1 <13.0.0",
    "yarn": ">=1.17.3 <2.0.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "storybook": "start-storybook -p 8001 --docs"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/cycleseven/cy7"
  },
  "eslintConfig": {
    "extends": [
      "@dangerfarms/eslint-config-df",
      "prettier"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal",
              "parent",
              "sibling",
              "index"
            ]
          ],
          "newlines-between": "always"
        }
      ],
      "import/no-unresolved": "off"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-styled-components",
      "stylelint-config-prettier"
    ],
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "rules": {
      "selector-type-case": null,
      "selector-type-no-unknown": null
    }
  },
  "prettier": {
    "endOfLine": "lf"
  }
}
