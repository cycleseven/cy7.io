{
  "name": "cy7.io",
  "private": true,
  "description": "My cool website!!",
  "version": "0.1.0",
  "author": "Owen Gillespie <owen@cy7.io>",
  "dependencies": {
    "@arkweid/lefthook": "0.7.0",
    "@babel/core": "7.8.3",
    "@dangerfarms/eslint-config-df": "5.0.0",
    "@mdx-js/mdx": "1.5.1",
    "@mdx-js/react": "1.5.1",
    "@storybook/addon-actions": "5.3.8",
    "@storybook/addon-docs": "5.3.8",
    "@storybook/react": "5.3.8",
    "@svgr/webpack": "^5.1.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "babel-preset-gatsby": "0.2.27",
    "date-fns": "2.9.0",
    "eslint": "6.2.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-cypress": "2.6.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.15.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-unicorn": "10.0.0",
    "gatsby": "2.19.3",
    "gatsby-image": "2.2.39",
    "gatsby-plugin-extract-schema": "0.0.5",
    "gatsby-plugin-manifest": "2.2.38",
    "gatsby-plugin-mdx": "1.0.67",
    "gatsby-plugin-offline": "3.0.32",
    "gatsby-plugin-react-helmet": "3.1.21",
    "gatsby-plugin-sharp": "2.4.0",
    "gatsby-plugin-styled-components": "3.1.18",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-typography": "2.3.21",
    "gatsby-remark-prismjs": "3.3.30",
    "gatsby-source-filesystem": "2.1.46",
    "gatsby-transformer-remark": "2.6.48",
    "gatsby-transformer-sharp": "2.3.13",
    "jest": "25.1.0",
    "polished": "3.4.2",
    "prettier": "1.19.1",
    "prism-react-renderer": "1.0.2",
    "prismjs": "1.19.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "styled-components": "5.0.0",
    "stylelint": "13.0.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-standard": "19.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.9.0",
    "typography": "0.16.19"
  },
  "engines": {
    "node": ">=12.9.1 <13.0.0",
    "yarn": ">=1.17.3 <2.0.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "test": "jest",
    "storybook": "start-storybook -p 8001 --docs"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/cycleseven/cy7"
  },
  "eslintConfig": {
    "extends": [
      "@dangerfarms/eslint-config-df",
      "prettier"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal",
              "parent",
              "sibling",
              "index"
            ]
          ],
          "newlines-between": "always"
        }
      ],
      "import/no-unresolved": "off"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "<rootDir>/jestPreprocess.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.cache",
      "/public/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-styled-components",
      "stylelint-config-prettier"
    ],
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "rules": {
      "selector-type-case": null,
      "selector-type-no-unknown": null
    }
  },
  "prettier": {
    "endOfLine": "lf"
  }
}
