{
  "name": "cy7.io",
  "private": true,
  "description": "My cool website!!",
  "version": "0.1.0",
  "author": "Owen Gillespie <owen@cy7.io>",
  "dependencies": {
    "@arkweid/lefthook": "0.6.3",
    "@dangerfarms/eslint-config-df": "5.0.0",
    "babel-plugin-styled-components": "1.10.6",
    "eslint": "6.2.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-cypress": "2.6.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.15.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-unicorn": "10.0.0",
    "gatsby": "2.13.79",
    "gatsby-image": "2.2.13",
    "gatsby-plugin-manifest": "2.2.9",
    "gatsby-plugin-offline": "2.2.10",
    "gatsby-plugin-react-helmet": "3.1.5",
    "gatsby-plugin-sharp": "2.2.17",
    "gatsby-plugin-styled-components": "3.1.3",
    "gatsby-source-filesystem": "2.1.16",
    "gatsby-transformer-sharp": "2.2.11",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "styled-components": "4.3.2",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/cycleseven/cy7"
  },
  "eslintConfig": {
    "extends": [
      "@dangerfarms/eslint-config-df",
      "prettier"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "groups": [
            [
              "builtin",
              "external"
            ],
            [
              "internal",
              "parent",
              "sibling",
              "index"
            ]
          ],
          "newlines-between": "always"
        }
      ]
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-styled-components",
      "stylelint-config-prettier"
    ],
    "processors": [
      "stylelint-processor-styled-components"
    ]
  },
  "prettier": {
    "endOfLine": "lf"
  }
}
