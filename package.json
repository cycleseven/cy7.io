  "author": "Owen Gillespie <owen@cy7.io>",
  "dependencies": {
    "@arkweid/lefthook": "0.7.2",
    "@babel/core": "7.12.13",
    "@babel/preset-typescript": "7.14.5",
    "@dangerfarms/eslint-config-df": "6.2.0",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@stitches/react": "0.2.2",
    "@storybook/addon-actions": "6.1.17",
    "@storybook/addon-docs": "6.1.17",
    "@storybook/react": "6.1.17",
    "@styled-system/css": "5.1.5",
    "@types/jest": "26.0.23",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-helmet": "6.1.0",
    "@types/react-router-dom": "5.1.7",
    "@types/styled-components": "5.1.10",
    "@types/typography": "0.16.3",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.12.0",
    "babel-preset-gatsby": "0.12.1",
    "browserslist": "4.16.3",
    "date-fns": "2.17.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "27.0.0",
    "gatsby": "2.32.3",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-extract-schema": "0.0.5",
    "gatsby-plugin-manifest": "2.12.0",
    "gatsby-plugin-mdx": "1.10.0",
    "gatsby-plugin-offline": "3.10.0",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-sharp": "2.14.1",
    "gatsby-plugin-styled-components": "3.10.0",
    "gatsby-plugin-typography": "2.12.0",
    "gatsby-remark-prismjs": "3.13.0",
    "gatsby-source-filesystem": "2.11.0",
    "gatsby-transformer-remark": "2.16.0",
    "gatsby-transformer-sharp": "2.12.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "prism-react-renderer": "1.1.1",
    "prismjs": "1.23.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.19",
    "styled-components": "5.2.1",
    "typescript": "4.3.4",
    "typography": "0.16.19"
  },
  "engines": {
    "node": ">=12.9.1 <13.0.0",
    "yarn": ">=1.17.3 <2.0.0"
  },
  "browserslist": [
    "defaults",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "test": "jest",
    "storybook": "start-storybook -p 8001 --docs"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/cycleseven/cy7"
  },
  "eslintConfig": {
    "extends": [
      "@dangerfarms/eslint-config-df",
      "prettier"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal",
              "parent",
              "sibling",
              "index"
            ]
          ],
          "newlines-between": "always"
        }
      ],
      "import/no-unresolved": "off"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)?$": "<rootDir>/jestPreprocess.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.cache",
      "/public/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "prettier": {
    "endOfLine": "lf"
  }
}
